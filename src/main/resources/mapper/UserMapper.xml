<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cosmos.mapper.UserMapper">
    <select id="selectByName" resultType="com.cosmos.pojo.User">
        select *from user where username = #{username}
    </select>
    <insert id="addStu" parameterType="com.cosmos.pojo.User">
        insert into user (username,password,role) values(#{username},#{password},#{role})
    </insert>
    <delete id="deleteStu" parameterType="string">
        delete from user where username = #{username}
    </delete>

</mapper>