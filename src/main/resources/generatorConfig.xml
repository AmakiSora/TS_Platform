    <context id="default" targetRuntime="MyBatis3">
        &lt;!&ndash;optional,指在创建class时，对注释进行控制&ndash;&gt;
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            &lt;!&ndash; 是否去除自动生成的注释 true：是 ： false:否 &ndash;&gt;
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        &lt;!&ndash;jdbc的数据库连接 wg_insert 为数据库名字&ndash;&gt;
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/TS_Platform?useUnicode=true&amp;characeterEncoding=utf-8&amp;serverTimezone=UTC"
                        userId="root"
                        password="lin961737902"/>
        &lt;!&ndash;非必须，类型处理器，在数据库类型和java类型之间的转换控制&ndash;&gt;
        <javaTypeResolver>
            &lt;!&ndash; 默认情况下数据库中的 decimal，bigInt 在 Java 对应是 sql 下的 BigDecimal 类 &ndash;&gt;
            &lt;!&ndash; 不是 double 和 long 类型 &ndash;&gt;
            &lt;!&ndash; 使用常用的基本类型代替 sql 包下的引用类型 &ndash;&gt;
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        &lt;!&ndash; targetPackage：生成的实体类pojo所在的包 &ndash;&gt;
        &lt;!&ndash; targetProject：生成的实体类pojo所在的硬盘位置 &ndash;&gt;
        <javaModelGenerator targetPackage="com.cosmos.pojo"
                            targetProject="src/main/java">
            &lt;!&ndash; 是否允许子包 &ndash;&gt;
            <property name="enableSubPackages" value="false"/>
            &lt;!&ndash; 是否对modal添加构造函数 &ndash;&gt;
            <property name="constructorBased" value="true"/>
            &lt;!&ndash; 是否清理从数据库中查询出的字符串左右两边的空白字符 &ndash;&gt;
            <property name="trimStrings" value="true"/>
            &lt;!&ndash; 建立modal对象是否不可改变 即生成的modal对象不会有setter方法，只有构造方法 &ndash;&gt;
            <property name="immutable" value="false"/>
        </javaModelGenerator>
        &lt;!&ndash; targetPackage 和 targetProject：生成的 mapper 文件的包和位置 &ndash;&gt;
        <sqlMapGenerator targetPackage="com.cosmos.mapper"
                         targetProject="src/main/java">
            &lt;!&ndash; 针对数据库的一个配置，是否把 schema 作为字包名 &ndash;&gt;
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>
        &lt;!&ndash; targetPackage 和 targetProject：生成的 interface 文件的包和位置 &ndash;&gt;
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.cosmos.dao" targetProject="src/main/java">
            &lt;!&ndash; 针对 oracle 数据库的一个配置，是否把 schema 作为字包名 &ndash;&gt;
            <property name="enableSubPackages" value="false"/>
        </javaClientGenerator>
        &lt;!&ndash; tableName是数据库中的表名，domainObjectName是生成的JAVA模型名，后面的参数不用改，要生成更多的表就在下面继续加table标签 &ndash;&gt;
        <table tableName="administrators" domainObjectName="administrators"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false"/>

    </context>
</generatorConfiguration>-->

